{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET 10 Unit Test Debug",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/backend/services/OrderService/tests/OrderService.UnitTests/bin/Debug/net10.0/OrderService.UnitTests.dll",
      "args": [
        "--filter",
        "FullyQualifiedName~OrderService.UnitTests.OrderServiceTest.CreateOrder_Should_Create_Order_When_Valid"
      ],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false
    }
  ]
}
